{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Запуск сервера",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/server", 
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",            
            "environment": [],
            "externalConsole": false,              
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
